{
  "expo": {
    "name": "june-voice-clean",
    "slug": "june-voice-clean",
    "version": "1.0.0",
    "scheme": "june",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "userInterfaceStyle": "automatic",
    "updates": {
      "url": "https://u.expo.dev/00000000-0000-0000-0000-000000000000"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kkazuma.junevoiceclean"
    },
    "android": {
      "package": "com.kkazuma.junevoiceclean",
      "permissions": [
        "RECORD_AUDIO",
        "CAMERA",
        "MODIFY_AUDIO_SETTINGS",
        "BLUETOOTH",
        "BLUETOOTH_CONNECT"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "@livekit/react-native-expo-plugin",
        {
          "androidCameraPermission": "Allow June Voice to access the camera",
          "androidRecordAudioPermission": "Allow June Voice to record audio"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "00000000-0000-0000-0000-000000000000"
      }
    }
  }
}